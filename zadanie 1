package com.company;

import java.util.Arrays;
import java.util.Random;
import java.util.Scanner;


public class Main {

    public static void main(String[] args) {

        Random rnd = new Random();

        int[] array = new int[10];
        for (int i = 0; i < array.length; i++) {
            array[i] = rnd.nextInt(15);
        }
        for (int i : array) {
            System.out.print(i + " ");
        }

    }

}
